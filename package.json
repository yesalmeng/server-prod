{
	"name": "yslm-server",
	"version": "1.0.14",
	"private": true,
	"description": "YSLM Backend Server",
	"repository": {
		"type": "git",
		"url": "https://github.com/edwinhern/express-typescript-2024"
	},
	"license": "MIT",
	"main": "index.ts",
	"scripts": {
		"build": "tsc && tsup",
		"check": "biome check --write",
		"start:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register src/index.ts",
		"start:prod": "NODE_ENV=production node dist/index.js",
		"test": "vitest run",
		"test:cov": "vitest run --coverage",
		"db:pull": "npx prisma db pull",
		"db:generate": "npx prisma generate",
		"supabase:setup": "npx supabase db push --local",
		"branch:setup": "pnpm run supabase:setup && pnpm run db:pull && pnpm run db:generate",
		"prepare": "husky install && npx prisma generate",
		"import:members": "NODE_ENV=development npx ts-node -r tsconfig-paths/register scripts/import-members.ts",
		"obfuscate": "npx tsx scripts/obfuscate.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.983.0",
		"@aws-sdk/s3-request-presigner": "^3.983.0",
		"@faker-js/faker": "^10.3.0",
		"@nestjs/cache-manager": "^3.0.1",
		"@nestjs/common": "^10.4.20",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^10.4.20",
		"@nestjs/platform-express": "^10.4.20",
		"@nestjs/swagger": "^7.4.2",
		"@nestjs/throttler": "^6.5.0",
		"@prisma/client": "^6.19.1",
		"@types/jsonwebtoken": "^9.0.10",
		"cache-manager": "^7.2.6",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.3",
		"cookie-parser": "^1.4.7",
		"cors": "2.8.5",
		"csv-parser": "^3.2.0",
		"dayjs": "^1.11.18",
		"dotenv": "17.2.1",
		"express": "5.1.0",
		"express-rate-limit": "8.0.1",
		"google-auth-library": "^10.3.0",
		"helmet": "8.1.0",
		"http-status-codes": "2.3.0",
		"jsonwebtoken": "^9.0.2",
		"pino": "9.9.0",
		"pino-http": "10.5.0",
		"prisma": "^6.19.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2",
		"swagger-ui-express": "5.0.1",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"@swc-node/register": "^1.11.1",
		"@swc/core": "^1.15.3",
		"@swc/helpers": "^0.5.17",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "2.8.19",
		"@types/express": "5.0.3",
		"@types/node": "^24.0.13",
		"@types/supertest": "6.0.3",
		"@types/swagger-ui-express": "4.1.8",
		"@vitest/coverage-v8": "3.2.4",
		"husky": "^8.0.0",
		"pino-pretty": "13.1.1",
		"supabase": "^2.39.2",
		"supertest": "7.1.4",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tsup": "8.5.0",
		"tsx": "4.20.4",
		"typescript": "5.9.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"packageManager": "pnpm@10.14.0",
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"outDir": "dist",
		"format": [
			"esm",
			"cjs"
		],
		"target": "es2020",
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"splitting": false,
		"skipNodeModulesBundle": true,
		"shims": true
	}
}